<h1>Users</h1>

<table>
  <thead>
    <th></th>
    <th>image</th>
    <th>name</th>
    <th colspan="3"></th>
  </thead>
  
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag @user, :profile_image, :fill,10,10,fallback: "no_image.jpg"%></td>
      <td><%= user.name %></td>  
      <td><%= link_to "Show","users/#{current_user.id}" %></td>
    <%end%>
    </tr>
  </tbody>
</table>